{
  "name": "nodejs-application",
  "type": "module",
  "scripts": {
    "start": "pnpm rebuild > /dev/null 2>&1 && pnpm build > /dev/null 2>&1 && node dist/main.js",
    "dev": "node --env-file=.env --no-warnings=ExperimentalWarning --import @swc-node/register/esm-register --watch src/main.ts",
    "build": "del dist/ && rollup -c",
    "test": "node --env-file=.env --no-warnings=ExperimentalWarning --import @swc-node/register/esm-register --test **/*.spec.ts",
    "lint": "eslint",
    "check-types": "tsc --noEmit",
    "prepare": "husky"
  },
  "main": "src/main.ts",
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@rollup/plugin-typescript": "^12.1.1",
    "@stylistic/eslint-plugin-js": "^2.9.0",
    "@stylistic/eslint-plugin-ts": "^2.9.0",
    "@swc-node/register": "^1.10.9",
    "@swc/cli": "0.4.1-nightly.20240914",
    "@swc/core": "^1.7.39",
    "@total-typescript/ts-reset": "^0.6.1",
    "@tsconfig/node22": "^22.0.0",
    "@tsconfig/strictest": "^2.0.5",
    "@types/eslint__js": "^8.42.3",
    "@types/lint-staged": "^13.3.0",
    "@types/node": "^22.7.8",
    "@types/user-agents": "^1.0.4",
    "del-cli": "^6.0.0",
    "eslint": "^9.13.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "rollup": "^4.24.0",
    "rollup-obfuscator": "^4.1.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.11.0"
  },
  "dependencies": {
    "pino": "^9.5.0",
    "robotjs": "^0.6.0",
    "zod": "^3.23.8"
  },
  "engines": {
    "node": "^22.2.0"
  },
  "packageManager": "pnpm@9.4.0"
}
